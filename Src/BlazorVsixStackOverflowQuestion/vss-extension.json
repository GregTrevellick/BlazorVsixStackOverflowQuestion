{
  "manifestVersion": 1,
  "id": "BlazorWidget",
  "version": "1.0.22",
  "name": "Blazor Widget",
  "publisher": "GregTrevellick",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "A work-in-progress / preview Blazor proof of concept widget",
  "icons": {
    "default": "wwwroot/Images/DefaultIcon.png"
  },
  "categories": [
    "Azure Boards "
  ],
  "tags": [
    "Blazor",
    "Widget"
  ],

  "links": {
    "home": {
      "uri": "https://marketplace.visualstudio.com/items?itemName=GregTrevellick.BlazorVsixStackOverflowQuestion"
    },
    "repository": {
      "uri": "https://github.com/GregTrevellick/BlazorVsixStackOverflowQuestion"
    }
  },
  "branding": {
    "color": "rgb(14,94,207)",
    "theme": "dark"
  },

  "contributions": [
    {
      "id": "JSParamValue_BlazorVsixStackOverflowQuestion",
      "type": "ms.vss-dashboards-web.widget",
      "targets": [
        "ms.vss-dashboards-web.widget-catalog"
      ],
      "properties": {
        "name": "Blazor Widget",
        "description": "A work-in-progress / preview Blazor proof of concept widget.",
        "catalogIconUrl:": "wwwroot/Images/CatalogIcon.png",
        "previewImageUrl": "wwwroot/Images/DefaultIcon.png",
        "uri": "wwwroot/index.html",
        "supportedSizes": [
          {
            "rowSpan": 1,
            "columnSpan": 2
          }
        ],
        "supportedScopes": [ "project_team" ]
      }
    }
  ],

  "files": [
    {
      "path": "wwwroot/index.html",
      "addressable": true
    },
    {
      "path": "wwwroot/Images",
      "addressable": true
    },
    {
      "path": "wwwroot/Scripts",
      "addressable": true
    },
    {
      "path": "bin/Release/netstandard2.1/dist/_framework/_bin",
      "addressable": true
    },
    {
      "path": "bin/Release/netstandard2.1/dist/_framework/wasm/dotnet.js",
      "addressable": true
    },
    {
      "path": "bin/Release/netstandard2.1/dist/_framework/wasm/dotnet.wasm",
      "addressable": true,
      "contentType": "application/wasm"
    },
    {
      "path": "wwwroot/_framework/_bin",
      "addressable": true
    },
    {
      "path": "wwwroot/_framework/wasm/dotnet.js",
      "addressable": true
    },
    {
      "path": "wwwroot/_framework/wasm/dotnet.wasm",
      "addressable": true,
      "contentType": "application/wasm"
    },
    {
      "path": "bin/Release/netstandard2.1/dist/_framework/blazor.boot.json",
      "addressable": true,
      "contentType": "application/json"
    },
    {
      "path": "wwwroot/_framework/blazor.boot.json",
      "addressable": true,
      "contentType": "application/json"
    },
    {
      "path": "bin/Release/netstandard2.1/dist/_framework/blazor.webassembly.js",
      "addressable": true
    },
    {
      "path": "wwwroot/_framework/blazor.webassembly.js",
      "addressable": true
    }
  ],

  "content": {
    "details": {
      "path": "wwwroot/Images/Overview.md"
    },
    "license": {
      "path": "wwwroot/Images/Licence.md"
    }
  },

  "repository": {
    "type": "git",
    "uri": "https://github.com/GregTrevellick/BlazorVsixStackOverflowQuestion"
  },

  "public": true,
  
  "galleryFlags": [
    "Preview"
  ]
  
}
